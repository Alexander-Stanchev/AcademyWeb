@model Academy.Web.Areas.Administration.Models.AdminViewModel
@{
    Layout = "_Layout";
}

<div class="row">
    <div class="col-md-12">
        <ul>
            List of all students:

            @foreach (var user in Model.Users)
            {
                <h1>@user.FullName</h1>
            }
        </ul>
    </div>
</div>

<div class="container">
    <a asp-area="" asp-controller="Dashboard" asp-action="Index" class="btn btn-block btn-lg btn-primary">Promote to Teacher</a>
</div>
